<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        <script src="~/Scripts/angular.js"></script>
    

  

    <script src="~/Scripts/agentclient.js"></script>
    <script src="~/Scripts/Controller/HomeController.js"></script>
    <script src="~/Scripts/Controller/customercontroller.js"></script>
    <script src="~/Scripts/Service/service.js"></script>
    
    <script src="~/Scripts/Service/agentservice.js"></script>
    <script src="~/Scripts/Controller/agentcontroller.js"></script>
    <script src="~/Scripts/Service/photoservice.js"></script>
    <script src="~/Scripts/Controller/photocontroller.js"></script>
    <script src="~/Scripts/Service/customerLogservice.js"></script>
    <script src="~/Scripts/Controller/customerLoginController.js"></script>
    <script src="~/Scripts/Service/venueservice.js"></script>
    <script src="~/Scripts/Controller/venuecontroller.js"></script>
    <script src="~/Scripts/Controller/adminlogincontroller.js"></script>
    <script src="~/Scripts/Controller/allbookingdetailsforadmincontroller.js"></script>
    <script src="~/Scripts/Controller/alldetailsforvendortoreferagentcontroller.js"></script>
    <script src="~/Scripts/Controller/detailsforadmintoacceptvendorcontroller.js"></script>
    <script src="~/Scripts/Controller/reviewbasedonvenueidcontoller.js"></script>
    <script src="~/Scripts/Service/vendorservice.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/Controller/vendorcontroller.js"></script>
    <script src="~/Scripts/Controller/customercontroller.js"></script>
    <script src="~/Scripts/Service/customerservice.js"></script>
    <script src="~/Scripts/Service/adminservice.js"></script>
    <script src="~/Scripts/Service/gautservice.js"></script>
</head>
<body ng-app="eventdbapp">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("OEMS", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="#home">Home</a></li>
                    <li><a href="#customerlogin">Login</a></li>
                    <li><a href="#custreg">SignUp</a></li>
                    <li><a href="#vendor">Vendor</a></li>
                    <li><a href="#searchby">Search venues </a></li>
                    <li><a href="#eventhome">View all venue </a></li>
                   

                    <li></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
